$("#EventDialog").dialog({
  autoOpen: true,
  height: 450,
  width: 700,
  modal: true,
  
	buttons: { 
		"Cancel": {  
			text: 'Discard', 
    	class: 'btn-red', 
    		click: function () {
					$(this).dialog("close");
			}
		},
		"Create": {  
			text: 'Create', 
    	class: 'btn-blue', 
    		click: function () {
					$(".docketForm").submit();
			}
		}
	},

	open: function() {
		$("#DocketForm").html("<%= escape_javascript(render(:partial => 'form')) %>")
		$(".ui-dialog-titlebar-close").hide();
	}
});