<%= form_for @docket, :html => { :class => "docketForm"}, :remote => true do |f| %>

  <p>
      <%= f.label :title %>
      <br/>
      <%= f.text_field :title %>
  </p>
  <p>
	  <%=f.label :description %>
  	  <br/>
  	  <%= f.text_area :description %>
  </p>
  <p>
	  <%=f.label :start_datetime, 'Start Time' %>
	  <br/>
   	  <%= f.datetime_select :start_datetime, :default => @parsed_start_datetime %>
  </p>
  <p>
	  <%=f.label :end_datetime, 'End Time' %>
  	  </br>
  	  <%= f.datetime_select :end_datetime, :default => @parsed_end_datetime %>
  </p>
  <%= f.hidden_field :users_id, :value => current_user.id unless current_user.id.blank? %>
  <% end %>