$("#EventDialog").dialog({
  autoOpen: true,
  height: 450,
  width: 700,
  modal: true,
  
	buttons: { 
		"Cancel": {  
			text: 'Back', 
    	class: 'btn-red', 
    		click: function () {
					$(this).dialog("close");
			}
		},
		"Edit": {  
			text: 'Edit', 
    	class: 'btn-blue',
    		click: function () {
				window.location = (" <%= escape_javascript(edit_docket_path(@docket))  %>");
			}
		}
	},

	open: function() {
		$("#DocketForm").html("<%= escape_javascript(render(:partial => 'form')) %>")
		$(".ui-dialog-titlebar-close").hide();
	},
});